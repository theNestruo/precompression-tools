{
	// See https://go.microsoft.com/fwlink/?LinkId=733558
	// for the documentation about the tasks.json format
	"version": "2.0.0",
	"tasks": [
		{
			"label": "mvn compile",
			"type": "shell",
			"command": "mvn --batch-mode compile",
			"group": {
				"kind": "build",
				"isDefault": true
			}
		}, {
			"label": "mvn package",
			"type": "shell",
			"command": "mvn --batch-mode clean package",
			"group": "build"
		}, {
			"label": "mvn verify",
			"type": "shell",
			"command": "mvn --batch-mode clean verify",
			"group": "build"
		}
	]
}
